{
  "name": "telar-cli",
  "version": "0.20.20",
  "description": "Telar development tools",
  "author": "amir@telar.dev",
  "bin": {
    "telar": "./bin/run.js"
  },
  "bugs": "https://github.com/Qolzam/telar-cli-js/issues",
  "dependencies": {
    "@fastify/deepmerge": "^1.3.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "close-with-grace": "^1.3.0",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fastify": "^4.27.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.12",
    "got": "^14.2.1",
    "html-minifier-terser": "^7.2.0",
    "hybrid-crypto-js": "^0.2.4",
    "inquirer": "^9.2.20",
    "is-docker": "^3.0.0",
    "js-yaml": "^4.1.0",
    "kleur": "^4.1.5",
    "nanospinner": "^1.1.0",
    "p-all": "^5.0.0",
    "package-up": "^5.0.0",
    "portfinder": "^1.0.32",
    "resolve-from": "^5.0.0",
    "semver": "^7.6.2",
    "socket.io": "^4.7.5",
    "tfastify-cli": "^6.1.1",
    "tiged": "^2.12.7",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/chalk-animation": "^1.6.3",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/yargs-parser": "^21.0.3",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "patch-package": "^8.0.0",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Qolzam/telar-cli-js",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "telar",
    "dirname": "telar",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "Qolzam/telar-cli-js",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && npm run copy:run:server && npm run copy:fastify",
    "lint": "eslint . --ext .ts",
    "copy:run:server": "shx mkdir -p ./dist/pkg/run/server && shx cp ./src/pkg/run/server/index.js ./dist/pkg/run/server/index.js",
    "copy:fastify": "shx cp ./src/pkg/fastify/lib/watch/fork.js ./dist/pkg/fastify/lib/watch/fork.js",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
