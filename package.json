{
  "name": "telar-cli",
  "version": "0.7.0",
  "description": "Telar necessary tools",
  "author": "amir",
  "bin": {
    "telar": "./bin/run"
  },
  "homepage": "https://github.com/Qolzam/telar-cli-js",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Qolzam/telar-cli-js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "fastify-cli": "^4.2.0",
    "fs-extra": "^10.1.0",
    "html-minifier-terser": "^7.0.0-alpha.2",
    "http-proxy": "^1.18.1",
    "js-yaml": "^4.1.0",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/html-minifier-terser": "^6.1.0",
    "@types/http-proxy": "^1.17.9",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/shelljs": "^0.8.11",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "telar",
    "dirname": "telar",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && cp src/pkg/run/server/package.json dist/pkg/run/server/package.json",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Qolzam/telar-cli-js/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
